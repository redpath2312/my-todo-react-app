{
	"name": "my-todo-react-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"check:branch:main": "node -e \"process.exit(require('child_process').execSync('git rev-parse --abbrev-ref HEAD').toString().trim()==='main'?0:1)\"",
		"check:branch:prodprep": "node -e \"process.exit(require('child_process').execSync('git rev-parse --abbrev-ref HEAD').toString().trim()==='prod-prep'?0:1)\"",
		"check:clean": "node -e \"process.exit(require('child_process').execSync('git status --porcelain').toString().length===0?0:1)\"",
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint .",
		"build:devpreview": "vite build --mode devpreview",
		"preview": "vite preview",
		"deploy:dev:preview": "npm run build:devpreview && firebase hosting:channel:deploy preview-dev --expires 7d --project dash-dev",
		"predeploy:prod:preview": "npm run lint && npm run build && npm run check:branch:prodprep",
		"deploy:prod:preview": "npm run predeploy:prod:preview && firebase hosting:channel:deploy prod-preview --expires 7d --project dash-prod",
		"deploy:prod:preview:force": "npm run check:branch:prodprep && npm run build && firebase hosting:channel:deploy prod-preview --project dash-prod --expires 7d",
		"predeploy:prod:live": "npm run lint && npm run build && npm run check:branch:main && npm run check:clean",
		"deploy:prod:live": "npm run predeploy:prod:live && firebase deploy --only hosting --project dash-prod",
		"rules:dev": "firebase deploy --only firestore:rules --project dash-dev",
		"rules:prod": "firebase deploy --only firestore:rules --project dash-prod",
		"indexes:dev": "firebase deploy --only firestore:indexes --project dash-dev",
		"indexes:prod": "firebase deploy --only firestore:indexes --project dash-prod",
		"channels:list:dev": "firebase hosting:channel:list --project dash-dev",
		"channels:list:prod": "firebase hosting:channel:list --project dash-prod",
		"open:dev:preview": "firebase hosting:channel:open preview-dev --project dash-dev",
		"open:prod:preview": "firebase hosting:channel:open prod-preview --project dash-prod"
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/icons-material": "^6.1.6",
		"@mui/material": "^6.1.6",
		"firebase": "^11.1.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^7.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.9.0",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.9",
		"globals": "^15.9.0",
		"postcss": "^8.5.3",
		"rollup-plugin-visualizer": "^6.0.3",
		"tailwindcss": "3.4.3",
		"vite": "^5.4.1"
	}
}
